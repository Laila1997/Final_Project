require 'nokogiri'
require 'open-uri'
require 'pry'
	html = open('https://twitter.com/JaneJfried')
	jff_app = Nokogiri::HTML(jff_html)
	
	projects = {}

	jff.css("div.ProfileTweet-contents p.ProfileTweet-text").each do |project|
	  tweet = project.css("div.ProfileTweet-contents p.").text.to_sym
	  projects[tweet] = {
	  	:image => project.css("div.project-thumbnail a img").attribute("src").value,
   		
	  }
	end
binding.pry